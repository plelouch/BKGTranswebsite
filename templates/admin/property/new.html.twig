{% extends 'admin/base.html.twig' %}

{% block title %}Nouvelle propriété {% endblock %}

{% block body %}
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Gestion des propriété</a>
            </li>
            <li class="breadcrumb-item active">Ajouter propriété</li>
        </ol>

        <div class="card mb-3">
            <div class="card-header">
                <i class="fas fa-plus"></i>
                Ajout d'une nouvelle propriété
            </div>
            <div class="card-body">
                {{ include('admin/property/_form.html.twig') }}
            </div>
        </div>
        <a class="btn btn-link" href="{{ path('admin_property_index') }}">
            <i class="fas fa-arrow-circle-left"></i>
            Retour à la liste
        </a>
    </div>
{% endblock %}
